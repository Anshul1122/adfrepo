{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "flatten1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     parameter1 as string",
				"}",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> source1",
				"source1 foldDown(unroll(123),",
				"     mapColumn(",
				"          first_name,",
				"          last_name,",
				"          location,",
				"          age = {123}.name",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten1"
			]
		}
	}
}